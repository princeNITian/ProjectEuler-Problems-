public class Main
{
	public static void main(String[] args) {
	//	System.out.println("Hello World");
	int num =1000;
	int sum=0;
	int arr3[]=new int[333];
	int arr5[]=new int[200];
	int arr15[]=new int[66];
	for(int i=0;i<333;i++){
	    arr3[i]=(i+1)*3;
	}
	
	for(int i=0;i<333;i++){
	sum=sum+arr3[i];
	//System.out.println(arr3[i])  ;
	}
	for(int i=0;i<200;i++){
	arr5[i]=i*5;
	sum=sum+arr5[i];
	//System.out.println(arr5[i]);
	}
	for(int i=0;i<66;i++){
	arr15[i]=(i+1)*15;
	sum=sum-arr15[i];
	}
	System.out.println("sum = "+sum);
	
	}
}
